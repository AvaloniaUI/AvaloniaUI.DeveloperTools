<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
        <add key="avalonia-pro" value="https://pro-nuget-feed.avaloniaui.net/v3/index.json" />
    </packageSources>
    <packageSourceCredentials>
        <avalonia-pro>
            <add key="Username" value="<YOUR_PORTAL_USERNAME>" />
            <add key="ClearTextPassword" value="<YOUR_PORTAL_LICENSE_KEY>" />
        </avalonia-pro>
    </packageSourceCredentials>
    <packageSourceMapping>
        <packageSource key="nuget.org">
            <package pattern="*" />
        </packageSource>
        <packageSource key="avalonia-pro">
            <package pattern="AvaloniaUI.*" />
        </packageSource>
    </packageSourceMapping>
</configuration>